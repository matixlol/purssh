{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "purssh",
	"compatibility_date": "2025-09-02",
	"compatibility_flags": [
		"nodejs_compat",
		"global_fetch_strictly_public"
	],
	"main": "src/worker.ts",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": [
			"*/15 * * * *"
		]
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "purssh",
			"database_id": "3eb319cf-dff6-4256-9cae-25da1490f064",
			"migrations_dir": "./migrations"
		}
	],
	"queues": {
		"producers": [
			{
				"binding": "NOTIFY_QUEUE",
				"queue": "purssh-notify"
			}
		],
		"consumers": [
			{
				"queue": "purssh-notify",
				"max_batch_size": 25,
				"max_batch_timeout": 5,
				"max_retries": 5
			}
		]
	},
	"vars": {
		"VAPID_PUBLIC_KEY": "BHp3saHAhdcI9ZRZUIxIeFHv3riuuYxVUFw6qI0CWF9Z3ahjm9dj13iS5QlVkzNr5YfmRyxvGagv77O5NBx4YHc",
		"VAPID_SUBJECT": "mailto:hola@mati.lol"
	}
	/**
